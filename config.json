        "language": "en-US",
        "engine": "microsoft"
      }
    },
    {
      "name": "JP->繁体字",
      "recognition_language": "ja",
      "xso_notification": false,
      "translation_param": {
        "slot": 1,
        "language": "zh-TW",
        "engine": "google"
      }
    },
    {
      "name": "JP->Português",
      "recognition_language": "ja",
      "xso_notification": false,
      "translation_param": {
        "slot": 1,
        "language": "p-PT",
        "engine": "google"
      }
    },
    {
      "name": "JP->Suomi(Finland)",
      "recognition_language": "ja",
      "xso_notification": false,
      "translation_param": {
        "slot": 1,
        "language": "fi-FI",
        "engine": "google"
      }
    },
    {
      "name": "JP->韓国語",
      "recognition_language": "ja",
      "xso_notification": false,
      "translation_param": {
        "slot": 1,
        "language": "ko-KR",
        "engine": "google"
      }
    },
    {
      "name": "JP->English(US)Google",
      "recognition_language": "ja",
      "xso_notification": true,
      "translation_param": {
        "slot": 1,
        "language": "en-US",
        "engine": "google"
      }
    }
  ],
  "debug": false
}
